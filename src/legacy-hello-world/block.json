{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bdm/legacy-hello-world",
	"version": "0.1.0",
	"title": "Legacy Hello World",
	"category": "block-development-mastery",
	"supports": {
		"__experimentalBorder": {
			"color": true,
			"style": true,
			"__experimentalDefaultControls": {
				"color": true,
				"style": true
			}
		},
		"__experimentalLayout": true,
		"align": true,
		"color": {
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"html": false,
		"spacing": {
			"blockGap": true,
			"margin": [ "top", "bottom" ],
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		}
	},
	"attributes": {
		"message": {
			"type": "string",
			"source": "html",
			"selector": ".wp-block-bcb-hello-world__message",
			"default": ""
		}
	},
	"editorScript": "file:index.js",
	"style": "file:style-index.css"
}
